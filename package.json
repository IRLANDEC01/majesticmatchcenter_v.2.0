  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "migrate": "migrate-mongo up",
    "migrate:create": "migrate-mongo create",
    "test": "cross-env CACHE_DRIVER=memory vitest run --config ./vitest.config.server.mjs",
    "test:server": "vitest --config vitest.config.server.mjs",
    "test:client": "vitest --config vitest.config.client.mjs",
    "test:watch": "vitest --config ./vitest.config.server.mjs",
    "test:ui": "vitest --ui --config ./vitest.config.server.mjs",
    "coverage": "vitest run --coverage",
    "db:seed": "tsx --tsconfig tsconfig.scripts.json -r dotenv/config scripts/seed-db.ts",
    "search:reindex": "tsx --tsconfig tsconfig.scripts.json -r dotenv/config scripts/seed-meilisearch.ts",
    "queue:clear:search": "tsx --tsconfig tsconfig.scripts.json -r dotenv/config scripts/clear-search-queue.ts",
    "start:dev": "npm run dev",
    "start:worker": "tsx --tsconfig tsconfig.scripts.json -r dotenv/config src/queues/search-worker.ts",
    "start:queue-monitor": "tsx --tsconfig tsconfig.scripts.json -r dotenv/config scripts/run-queue-monitor.ts"
